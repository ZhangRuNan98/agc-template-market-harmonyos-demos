{
  "module": {
    "name": "video_swiper",
    "type": "shared",
    "deviceTypes": [
      "phone"
    ],
    "installationFree":true,
    'deliveryWithInstall': true,
    "requestPermissions": [
      {
        "name": "ohos.permission.KEEP_BACKGROUND_RUNNING",
        "reason": "$string:reason_background",
        "usedScene": {
          "abilities": [
            "EntryAbility"
          ],
          "when": "always"
        }
      }
    ]
  }
}