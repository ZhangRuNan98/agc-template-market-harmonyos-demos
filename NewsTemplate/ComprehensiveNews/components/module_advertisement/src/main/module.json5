{
  "module": {
    "name": "module_advertisement",
    "type": "har",
    "deviceTypes": [
      "default",
      "tablet",
      "2in1"
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET",
      },
      {
        "name": "ohos.permission.APP_TRACKING_CONSENT",
        "reason": "$string:reason",
        "usedScene": {
          "abilities": [
            "FormAbility",
            "EntryFormAbility"
          ],
          "when": "inuse"
        }
      }
    ]
  }
}