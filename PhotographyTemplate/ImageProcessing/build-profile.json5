{
  "app": {
    "signingConfigs": [],
    "products": [
      {
        "name": "default",
        "signingConfig": "default",
        "compatibleSdkVersion": "5.0.4(16)",
        "runtimeOS": "HarmonyOS",
        "buildOption": {
          "strictMode": {
            "caseSensitiveCheck": true,
            "useNormalizedOHMUrl": true
          }
        }
      }
    ],
    "buildModeSet": [
      {
        "name": "debug",
      },
      {
        "name": "release"
      }
    ]
  },
  "modules": [
    {
      "name": "phone",
      "srcPath": "./product/phone",
      "targets": [
        {
          "name": "default",
          "applyToProducts": [
            "default"
          ]
        }
      ]
    },
    {
      "name": "picture_beautification",
      "srcPath": "./scenes/picture_beautification",
    },
    {
      "name": "collage",
      "srcPath": "./scenes/collage",
    },
    {
      "name": "common",
      "srcPath": "./common",
    },
    {
      "name": "personal",
      "srcPath": "./scenes/personal",
    },
    {
      "name": "picture_collage",
      "srcPath": "./components/picture_collage",
    }
  ]
}